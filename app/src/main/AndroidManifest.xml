<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="brigitasugg.brigitasugg" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".createNewContact"
            android:label="@string/title_activity_create_new_contact" >
        </activity>
        <activity
            android:name=".NewContact"
            android:label="@string/title_activity_new_contact"
            android:parentActivityName=".createNewContact" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="brigitasugg.brigitasugg.createNewContact" />
        </activity>
    </application>

</manifest>
